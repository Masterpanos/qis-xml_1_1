  <contact>
    <text>“pascal.heus@gmail.com”</text>
  </contact>
  <description>
    <text>This module is a YANG version of the QIS-XML 1.1 specification.</text>
  </description>
  <revision date="2016-09-04">
    <description>
      <text>Draft/Development version</text>
    </description>
  </revision>
  <container name="qis">
    <description>
      <text>A QIS instance</text>
    </description>
